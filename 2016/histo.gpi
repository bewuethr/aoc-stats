set terminal png font 'Inconsolata, 14' background rgb '#002b36'
stats "histodata.txt" u 2 nooutput
set xrange [0:24]
set xlabel "Time of day [hours]" tc '#839496'
set yrange [0:STATS_max+10]
set ylabel "Successful submissions" tc '#839496'
set border lc rgb '#839496'
set grid lc rgb '#586e75'
set style fill solid
set boxwidth 0.85
plot "histodata.txt" u ($1+0.5):2 with boxes notitle lc rgb '#b58900'
